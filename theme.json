{
	"$schema": "https://raw.githubusercontent.com/WordPress/gutenberg/trunk/schemas/json/theme.json",
	"version": 2,
	"settings": {
		"layout": {
			"contentSize": "var(--wp--custom--content-size--normal)",
			"wideSize": "calc( var(--wp--custom--content-size--normal) + ( 100vw - var(--wp--custom--content-size--normal) ) / 2 )"
		},
		"color": {
			"background": true,
			"text": true,
			"link": false,
			"custom": true,
			"defaultPalette": true,
			"palette": [
				{
					"slug": "primary",
					"color": "#233A73",
					"name": "Primary"
				},
				{
					"slug": "primary-hover",
					"color": "#1c45ab",
					"name": "Primary Hover"
				},
				{
					"slug": "text-normal",
					"color": "#333",
					"name": "Text Normal "
				},
				{
					"slug": "text-secondary",
					"color": "rgba(0,0,0,0.5)",
					"name": "Text Secondary"
				},
				{
					"slug": "border-normal",
					"color": "rgba( 0,0,0,0.1 )",
					"name": "Border Normal"
				},
				{
					"slug": "bg-light-gray",
					"color": "#f9f9f9",
					"name": "BG Light Gray"
				},
				{
					"slug": "bg-dark-gray",
					"color": "#262626",
					"name": "BG Dark Gray"
				},
				{
					"slug": "bg-black",
					"color": "#222222",
					"name": "BG Black"
				},
				{
					"slug": "text-normal-darkbg",
					"color": "rgba( 255,255,255,0.8 )",
					"name": "Text Normal (Dark BG)"
				},
				{
					"slug": "text-secondary-darkbg",
					"color": "rgba( 255,255,255,0.6 )",
					"name": "Text Secondary (Dark BG)"
				},
				{
					"slug": "border-normal-darkbg",
					"color": "rgba( 255,255,255,0.1 )",
					"name": "Border Normal (Dark BG)"
				},
				{
					"slug": "text-bright",
					"color": "rgba(0,0,0,0.51)",
					"name": "Text Bright (Will be abolished)"
				},
				{
					"slug": "text-dark-darkbg",
					"color": "rgba( 255,255,255,0.61 )",
					"name": "Text Dark (Will be abolished)"
				}
			],
			"customGradient": true,
			"defaultGradients": true,
			"gradients": [],
			"duotone": [],
			"customDuotone": true
		},
		"spacing": {
			"blockGap": null,
			"margin": true,
			"padding": true,
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"typography": {
			"dropCap": false,
			"lineHeight": true,
			"fontFamilies": [
				{
					"slug": "system-font",
					"name": "System Font",
					"fontFamily": "\"ヒラギノ角ゴ ProN\", \"Hiragino Kaku Gothic ProN\", \"メイリオ\", Meiryo, sans-serif"
				},
				{
					"fontFamily": "\"Source Serif Pro\", serif",
					"name": "Source Serif Pro",
					"slug": "source-serif-pro",
					"fontFace": [
						{
							"fontFamily": "Source Serif Pro",
							"fontWeight": "200 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/source-serif-pro/SourceSerif4Variable-Roman.ttf.woff2"
							]
						},
						{
							"fontFamily": "Source Serif Pro",
							"fontWeight": "200 900",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [
								"file:./assets/fonts/source-serif-pro/SourceSerif4Variable-Italic.ttf.woff2"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"name": "Tiny",
					"slug": "tiny",
					"size": "10px"
				},
				{
					"name": "X small",
					"slug": "x-small",
					"size": "0.75rem"
				},
				{
					"name": "Small",
					"slug": "small",
					"size": "0.875rem"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "1rem"
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "1.125rem"
				},
				{
					"name": "X Large",
					"slug": "x-large",
					"size": "1.25rem"
				},
				{
					"name": "XX Large",
					"slug": "xx-large",
					"size": "1.5rem"
				},
				{
					"name": "XXX Large",
					"slug": "xxx-large",
					"size": "1.75rem"
				},
				{
					"name": "Huge",
					"slug": "huge",
					"size": "2.2rem"
				}
			]
		},
		"custom": {
			"content-size": {
				"normal": "min( 1110px, 90vw )"
			},
			"layout": {
				"sidebar": "220px"
			},
			"spacing": {
				"x-small": "calc(var(--wp--custom--spacing--medium) / 4)",
				"small": "calc(var(--wp--custom--spacing--medium) / 2)",
				"medium": "1.5rem",
				"large": "calc(var(--wp--custom--spacing--medium) * 1.5)",
				"x-large": "calc(var(--wp--custom--spacing--medium) * 2)",
				"editor-padding": "2rem",
				"button-sm": "0.3em 1em",
				"menu-indent": "0.875rem"
			},
			"typography": {
				"line-height": {
					"normal": "1.75em",
					"heading": "1.5em"
				}
			}
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)"
		},
		"color": {
			"background": "#fff",
			"text": "var(--wp--preset--color--text-normal)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontWeight": "700",
					"fontSize": "var(--wp--preset--font-size--huge)"
				},
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "0"
					}
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxx-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--text-normal)"
				}
			}
		},
		"blocks": {
			"core/navigation": {
				"typography": {
					"lineHeight": "1.5"
				}
			}
		}
	},
	"customTemplates": [
		{
			"name": "page-2col",
			"title": "Page 2 column",
			"postTypes": [
				"page"
			]
		},
		{
			"name": "page-lp",
			"title": "Page for LP",
			"postTypes": [
				"page"
			]
		},
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": [
				"page",
				"post"
			]
		}
	],
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "sidebar-post",
			"title": "Sidebar(Post)",
			"area": "uncategorized"
		}
	]
}