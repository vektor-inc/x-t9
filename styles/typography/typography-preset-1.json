{
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"title": "2025 Typography Preset",
	"slug": "x-t9-2025",
	"settings": {
		"typography": {
			"fontFamilies": [
				{
					"slug": "system-font",
					"name": "System Font",
					"fontFamily": "Helvetica Neue, Arial, \"Hiragino Kaku Gothic ProN\", \"Hiragino Sans\", Meiryo, sans-serif"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Tiny",
					"slug": "tiny",
					"size": "10px"
				},
				{
					"fluid": false,
					"name": "X small",
					"slug": "x-small",
					"size": "clamp(11px, calc(11px + (100vw - 360px) * ( (12 - 11) / (1200 - 360)) ), 12px)"
				},
				{
					"fluid": false,
					"name": "Small",
					"slug": "small",
					"size": "clamp(12.56px, calc( 12.56px + (100vw - 360px) * ( (13.91 - 12.56) / (1200 - 360)) ), 13.91px)"
				},
				{
					"name": "Medium",
					"slug": "medium",
					"size": "clamp(14px, calc( 14px + (100vw - 360px) * ( (16 - 14) / (1200 - 360)) ), 16px)"
				},
				{
					"name": "Large",
					"slug": "large",
					"size": "clamp(15.61px, calc( 15.61px + (100vw - 360px) * ( (18.4 - 15.61) / (1200 - 360)) ), 18.4px)"
				},
				{
					"name": "X Large",
					"slug": "x-large",
					"size": "clamp(17.4px, calc( 17.4px + (100vw - 360px) * ( (21.16 - 17.4) / (1200 - 360)) ), 21.16px)"
				},
				{
					"name": "XX Large",
					"slug": "xx-large",
					"size": "clamp(19.4px, calc( 19.4px + (100vw - 360px) * ( (24.33 - 19.4) / (1200 - 360)) ), 24.33px)"
				},
				{
					"name": "XXX Large",
					"slug": "xxx-large",
					"size": "clamp(21.64px, calc( 21.64px + (100vw - 360px) * ( (27.98 - 21.64) / (1200 - 360)) ), 27.98px)"
				},
				{
					"name": "Huge",
					"slug": "huge",
					"size": "clamp(24.12px, calc( 24.12px + (100vw - 360px) * ( (32.18 - 24.12) / (1200 - 360)) ), 32.18px)"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontFamily": "var:preset|font-family|source-serif-pro",
					"fontWeight": "900",
					"lineHeight": "1"
				}
			}
		},
		"elements": {
			"p": {
				"typography": {
					"fontFamily": "var:preset|font-family|system-font",
					"fontWeight": "400"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|source-serif-pro",
					"fontWeight": "900",
					"lineHeight": "1.2"
				}
			}
		}
	}
}
