{
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"title": "Serif Title And Nav",
	"slug": "serif-title-and-navi",
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"fontFamily": "var:preset|font-family|system-font-serif"
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var:preset|font-family|system-font-serif"
				}
			}
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|system-font-serif",
					"fontWeight": "900"
				}
			}
		}
	}
}
